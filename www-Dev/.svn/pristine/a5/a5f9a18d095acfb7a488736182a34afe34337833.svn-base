<%@ Control Language="VB" ClassName="PhoneNumber" CodeFile="PhoneNumber.ascx.vb" Inherits="controls_PhoneNumber" %>
<table cellpadding="0" cellspacing="0" style="width:1%">
  <tbody>
    <tr>
      <td colspan="5" style="width:1%"><asp:Label ID="lblPhoneNumber" Text="Phone Number" runat="server" /></td>
      <td style="width: 4px;">&nbsp;</td>
      <td style="width:1%" ><asp:Label ID="lblExtension" Text="Extension" runat="server" /></td>
      <td style="width: 4px;">&nbsp</td>
      <td style="width:1%" ><asp:Label ID="lblPin" Text="Pin" runat="server" /></td>
      <td style="width: 4px;">&nbsp</td>
      <td ><asp:Label ID="lblPhoneType" runat="server" Text="Type" /></td>
    </tr>
    <tr>
      <td style="white-space:nowrap">(<asp:TextBox style="width:30px;" maxlength="3" id="txtAreaCode" runat="server" />)</td>
      <td style="width: 4px;">&nbsp;</td>
      <td><asp:TextBox style="width:30px;" maxlength="3" ID="txtExchange" runat="server" /></td>
      <td style="width: 4px;">-</td>
      <td><asp:TextBox style="width:40px;" maxlength="4" ID="txtLineNumber" runat="server" /></td>
      <td style="width: 4px;">&nbsp;</td>
      <td><asp:TextBox style="width:80px;" maxlength="16" ID="txtExtension" runat="server" /></td>
      <td style="width: 4px;">&nbsp;</td>
      <td><asp:TextBox style="width:80px;" maxlength="16" ID="txtPin" runat="server" /></td>
      <td style="width: 4px;">&nbsp;</td>
      <td><asp:DropDownList runat="server" ID="cbxPhoneTypes" /></td>
    </tr>    
  </tbody>
</table>
