
/* CSS Document */

#navcontainer{/*no needed */}

/* pull-down mainmenu css */
#navlist{
	margin:0;
	padding:0;
	list-style-type: none;
	white-space:nowrap;
	width:100%;
	
}
#navlist li 
{
	display: block;
	float: left;
	font-family: Verdana, Arial, Sans-Serif ;
	font-size: 10px;
	font-weight: bold;
	margin:  0 0 0 0;
	padding: 1em 0 1em 0;
	background-color: #2647A0;
	border-top: 1px solid #2647A0;
	border-bottom: 1px solid #2647A0;
	
}

#navlist a, #navlist a:link {
	margin: 0;
	padding: 1em 9px 1em 9px;
	color: #ffffff;
	border-right: 1px solid #2647A0;
	text-decoration : none;
	
}


#navlist a:hover, #navlist a:focus {
	color: #ffff00;
	background-color: #BBD9EE;
}
#navlist a:active 
{
	color: #ffff00;
	background-color: #4f93e3;
	border-top: 1px solid #4f93e3;
	border-bottom: 1px solid #4f93e3;
}

.nav 
{
	background-color: #2647A0;
}
/* end of mainmenu css */

