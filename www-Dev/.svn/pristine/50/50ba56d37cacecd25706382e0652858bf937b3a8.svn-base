<%@ Control Language="VB" ClassName="Address" CodeFile="Address.ascx.vb" Inherits="controls_Address" %>
<table cellpadding="0" cellspacing="0" style="width: 90%; text-align: left;">
  <tbody>
    <tr>
      <td colspan="5">
        <div >
          <asp:Label ID="lblStreet" runat="server" /></div>
        <div style="padding-right: 1px;">
          <asp:TextBox Style="width: 98%" ID="txtStreet" runat="server" /></div>
        <div >
          <asp:Label ID="lblExtended" runat="server" /></div>
        <div style="padding-right: 1px;">
          <asp:TextBox Style="width: 98%" ID="txtExtended" runat="server" /></div>
        <div >
          <asp:Label ID="lblCity" runat="server" /></div>
        <div style="padding-right: 1px;">
          <asp:TextBox Style="width: 98%" ID="txtCity" runat="server" /></div>
      </td>
    </tr>
    <tr>
      <td style="width: 1%" class="label">
        <asp:Label ID="lblState" runat="server" /></td>
      <td style="width: 4px;">
        </td>
      <td class="label">
        <asp:Label ID="lblZip" runat="server" /></td>
      <td style="width: 4px;">
        </td>
      <td class="label">
        <asp:Label ID="lblAddressType" runat="server" />&nbsp;</td>
    </tr>
    <tr>
      <td style="width: 1%">
        <asp:DropDownList ID="cbxStates" runat="server" /></td>
      <td style="width: 4px;">
        </td>
      <td style="width: 1%">
        <asp:TextBox ID="txtZip" MaxLength="16" runat="server" /></td>
      <td style="width: 4px;">
        </td>
      <td>
        <asp:DropDownList Style="width: 98%" ID="cbxAddressTypes" runat="Server" />&nbsp;</td>
    </tr>
  </tbody>
</table>
