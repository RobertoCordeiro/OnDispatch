<%@ Control Language="VB" ClassName="DateTimePicker" CodeFile="DateTimePicker.ascx.vb" Inherits="controls_DateTimePicker" %>
<table style="border:0; padding: 0,0,0,0" cellpadding="0">
  <tbody>
    <tr>
      <td id="tdYear" runat="server" class="label">Year</td>
      <td id="tdMonth" runat="server" class="label">Month</td>
      <td id="tdDay" runat="server" class="label">Day</td>      
      <td>&nbsp;</td>
      <td id="tdHour" runat="server" class="label">Hour</td>
      <td id="tdMinute" runat="server" class="label">Minute</td>
    </tr>
    <tr>
      <td id="tdYearData" runat="server"><asp:DropDownList ID="cbxYears" runat="server" /></td>
      <td id="tdMonthData" runat="server"><asp:DropDownList ID="cbxMonths" runat="server" /></td>      
      <td id="tdDayData" runat="server"><asp:DropDownList ID="cbxDays" runat="server" /></td>
      <td>&nbsp;</td>
      <td id="tdHourData" runat="server"><asp:DropDownList ID="cbxHours" runat="server" /></td>
      <td id="tdMinuteData" runat="server"><asp:DropDownList ID="cbxMinutes" runat="server" /></td>
    </tr>    
  </tbody>  
</table>
